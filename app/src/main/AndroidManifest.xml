<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shankar.firebaseauthentication">

    <application
        android:allowBackup="true"
        android:icon="@drawable/firebase"
        android:label="@string/app_name"
        android:roundIcon="@drawable/firebase"
        android:supportsRtl="true"
        android:theme="@style/Theme.FirebaseAuthentication">
        <activity android:name=".Phone.PhoneA"></activity>
        <activity android:name=".EmailAndPassword.EmailAndPassword" />
        <activity android:name=".UserActivity" />
        <activity android:name=".Google.GoogleA">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>